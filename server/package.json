{
  "name": "questions-game-backend",
  "version": "1.0.0",
  "description": "Backend server for the Questions Game App",
  "main": "server.js",
  "scripts": {
    "create-dev-tables": "babel-node ./app/db/dbConnection createUserTable",
    "drop-dev-tables": "babel-node ./app/db/dbConnection dropUserTable",
    "start": "nodemon --watch . --exec babel-node -- server",
    "prod": "babel-node server.js",
    "setup": "npm-run-all -p prod create-dev-tables",
    "revamp": "npm-run-all -p start drop-dev-tables"
  },
  "keywords": [
    "Node",
    "docker",
    "Postgres"
  ],
  "author": "Alexander Nnakwue",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^7.0.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@hapi/joi": "^17.1.1",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.8",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "pg": "^8.4.1",
    "socket.io": "^2.3.0"
  },
  "engines": {
    "node": "10.x"
  }
}
